{
  "semi": true,
  "singleQuote": true,
  "bracketSameLine": true,
  "printWidth": 160,
  "tabWidth": 2,
  "useTabs": false,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "lf",
  "overrides": [
    {
      "files": "*.php",
      "options": {
        "tabWidth": 4,
        "printWidth": 160,
        "singleQuote": true
      }
    }
  ]
}
